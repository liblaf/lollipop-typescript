{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "allowCompoundWords": true,
  "enableGlobDot": true,
  "ignorePaths": [
    "**/.cspell.*",
    "**/.git/",
    "**/*-lock.*",
    "**/*.lock",
    "**/*.sum"
  ],
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "dotenv",
    "jscoverage",
    "jspm",
    "liblaf",
    "nuxt",
    "pids",
    "stefanzweifel",
    "tsup",
    "vuepress",
    "wscript"
  ]
}
